{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [{
    "name": "client",
    "image": "davidainslie/multi-client",
    "hostname": "client",
    "essential": false
  }, {
    "name": "api",
    "image": "davidainslie/multi-api",
    "hostname": "api",
    "essential": false  
  }, {
    "name": "worker",
    "image": "davidainslie/multi-worker",
    "essential": false
  }, {
    "name": "nginx",
    "image": "davidainslie/multi-nginx",
    "essential": true,
    "portMappings": [{
      "hostPort": 80,
      "containerPort": 80
    }],
    "links": ["client", "api"]
  }]
}